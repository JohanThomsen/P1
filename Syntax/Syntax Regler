  int variabel_navn_med_underscore = 0,
      brug_kommaer_hvis_muligt     = 0;
  int *pointer_stjerne_ved_variabel_navn;  /* Line up så det står under hinanden. Konkrete men helst hold længden nede. */

  if ((variabel_a > variabel_b) || (brug_parenteser_ved_logiske_udtryk)){

  } else {
    Else efter sidste 
  }
 
  if ((a != 0) || (boolsk_funktion())){
    Brug curly brackets. Selv ved one liners!!!
  }

  BRUG SWITCH HVIS FLERE END eller lig med 3 EFTERFØLGENDE IF STATEMENTS PÅ SAMME VARIABEL. (som i følgende eksempel):

  if (1 == a){

  }

  if (2 == a){

  }

  if (3 == a){

  }

  switch (a)
    case 1: 
      break;
    
    case 2:
      break;
    
    case 3:
      break;

    default:
      break;
}

